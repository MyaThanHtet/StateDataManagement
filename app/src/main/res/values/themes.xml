        <item name="colorPrimary">@color/login_button</item>
        <item name="colorPrimaryVariant">#FFFFFF</item>
        <item name="colorOnPrimary">#2196F3</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#03A9F4</item>
        <item name="colorSecondaryVariant">#03A9F4</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="android:toolbarStyle">@color/black</item>
